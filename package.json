{
  "name": "tntech-statcrew-webserver",
  "version": "1.0.1",
  "description": "Webserver to serve .xml files generated by statcrew for Tennessee Tech University's sports program",
  "main": "lib/index.js",
  "bin": "lib/index.js",
  "scripts": {
    "clean": "rimraf ./lib/* ./dts/*",
    "prebuild": "npm run clean",
    "build": "npm run clean && tsc -p src",
    "watch": "npm run clean && tsc -p src --watch",
    "prepublish": "rimraf ./bin/*",
    "publish": "npm run build && pkg --out-path ./bin/ --targets win-x86,linux --no-bytecode --public --public-packages '*' .",
    "start-dev": "node lib/index.js",
    "prepackage:linux": "npm run build",
    "package:linux": "pkg lib/index.js -o bin/tscw-daemon.bin -t node12-linux-x64 --no-bytecode --public --public-packages '*'",
    "prepackage:macos": "npm run build",
    "package:macos": "pkg lib/index.js -o bin/tscw-daemon -t node12-macos-x64 --no-bytecode --public --public-packages '*'",
    "prepackage:windows": "npm run build",
    "package:windows": "pkg lib/index.js -o bin/tscw-daemon.exe -t node12-windows-x86 --no-bytecode --public --public-packages '*'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyleratti/tntech-statcrew-webserver.git"
  },
  "author": "Kyle Ratti",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/kyleratti/tntech-statcrew-webserver/issues"
  },
  "homepage": "https://github.com/kyleratti/tntech-statcrew-webserver#readme",
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.0.8",
    "@types/node": "^13.13.32",
    "@types/socket.io-client": "^1.4.34",
    "http-status-codes": "^1.4.0",
    "pkg": "^4.4.9",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.3",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "fs": "0.0.1-security",
    "moment": "^2.29.1",
    "path": "^0.12.7",
    "socket.io": "^2.4.0"
  }
}
